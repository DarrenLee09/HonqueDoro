{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "angular",
  "outputDirectory": "dist/angular-app/browser",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.csr.html"
    }
  ]
}