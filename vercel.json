{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": false,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}